package HacherBlocks;

import java.util.Scanner;

//Longest Increasing Subsequence
public class LIS {
	public static void main(String[] args) {
		Scanner scr = new Scanner(System.in);
		int n = scr.nextInt();
		int a[] = new int[n];
		for (int i = 0; i < n; i++) {
			a[i] = scr.nextInt();
		}
		increasingSubs(a);
	}
}
