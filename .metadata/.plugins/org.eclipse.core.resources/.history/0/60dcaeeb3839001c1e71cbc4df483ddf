import java.util.ArrayList;
import java.util.List;

public class ArraysBasic {
	public static void main(String[] args) {
		int ar[] = new int[5];
		for (int i = 0; i < ar.length; i++) {
			ar[i] = i * 2;
		}
		for (int i : ar) {
			System.out.print(i);
		}
		List<Integer> a = new ArrayList<>();
	}
}
