package Amazon;

class Node {
	int dist;
	int []pt;
	Node(int dist,int []pt){
		this.dist=dist;
		this.pt=pt;
	}
}

public class kClosest {
	public static void main(String[] args) {
		int[][] ar = { { 1, 3 }, { -2, 2 } };
		kclosest(ar);
	}

	private static void kclosest(int[][] ar) {
		// TODO Auto-generated method stub
		for (int l = 0; l < ar.length; l++) {
			Node  nn=new Node();
			int i = ar[l][0];
			int j = ar[l][1];
			int s = i * i + j * j;
			
		}
	}
}
