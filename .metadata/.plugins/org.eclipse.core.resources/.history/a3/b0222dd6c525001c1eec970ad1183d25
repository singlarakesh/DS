import java.util.Scanner;

public class AtoB {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
       Scanner scr=new Scanner(System.in);
       String a=scr.next();
       String b=scr.next();
       int c=0;
       while(a.length()<b.length()) {
    	   a+=a;
    	   c++;
       }
       int i=0;
       int j=0;
       while(i<a.length() && j<b.length()) {
    	   if(a.charAt(i)==b.charAt(j)) {
    		   i++;
    		   j++;
    	   }else {
    		   i++;
    	   }
       }
	}

}
