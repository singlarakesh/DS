package Java8;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class Stream {
	public static void main(String[] args) {
//      int ar[]= {1,2,3,4};
      List<Integer> list=Arrays.asList(2,3,4,5);
      list.stream().map(i->i*i).collect(Collectors.toList());
	}
}
