package Pubmatic;

import java.util.List;

public class Producer implements Runnable {
	private List<String> buffer;

	public Producer(List<String> buffer) {
		this.buffer = buffer;
	}

	@Override
	public void run() {
		// TODO Auto-generated method stub
		String numbers[] = { "1", "2", "3" };
		for (String number : numbers) {
			buffer.add(number);
		}
	}

}
