package dp;

import java.util.Scanner;

public class Min_Cost_Climbing_Stairs {
	public static void main(String[] args) {
		Scanner scr = new Scanner(System.in);
		int n = scr.nextInt();
		int cost[] = new int[n];
		for (int i = 0; i < n; i++) {
			cost[i] = scr.nextInt();
		}
		int dp[] = new int[n + 1];
		dp[n] = cost[n - 1];
	}
}
